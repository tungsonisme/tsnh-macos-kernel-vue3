{
  "name": "tsnh-macos-kernel",
  "version": "1.3.0",
  "description": "Kernel of Macos.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tungsonisme/tsnh-macos-kernel-vue3.git"
  },
  "author": "tungsonisme",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tungsonisme/tsnh-macos-kernel-vue3/issues"
  },
  "homepage": "https://github.com/tungsonisme/tsnh-macos-kernel-vue3#readme",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/macos-kernel.umd.cjs",
  "module": "./dist/macos-kernel.js",
  "types": "./dist/exposes.d.js",
  "exports": {
    ".": {
      "import": "./dist/macos-kernel.js",
      "require": "./dist/macos-kernel.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "dev:prod": "nodemon --watch './src/*' -x \"pnpm build\" -e ts,vue",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "eslint": "eslint '**/*.{ts,vue}'",
    "stylelint": "stylelint '**/*.{scss,vue}'",
    "prepublishOnly": "pnpm run build"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "yarn eslint --quiet --cache --fix"
    ],
    "*.{css,scss,vue}": [
      "yarn stylelint --fix"
    ]
  },
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "pinia": "^2.0.28",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "eslint-scope": "^7.1.1",
    "eslint-utils": "^3.0.0",
    "eslint-visitor-keys": "^3.3.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "pinia": "^2.0.28",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.0",
    "prettier-linter-helpers": "^1.0.0",
    "sass": "^1.57.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-processor-html": "^1.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-css-injected-by-js": "^2.2.0",
    "vite-plugin-dts": "^1.7.1",
    "vue": "^3.2.45",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.11"
  }
}
